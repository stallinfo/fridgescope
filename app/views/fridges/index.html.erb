<p id="notice"><%= notice %></p>

<h1>Fridges</h1>

<table>
  <thead>
    <tr>
      <th>Facility</th>
      <th>Name</th>
      <th>Description</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Update count</th>
      <th>Created by</th>
      <th>Created api caller</th>
      <th>Updated by</th>
      <th>Updated api caller</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fridges.each do |fridge| %>
      <tr>
        <td><%= fridge.facility_id %></td>
        <td><%= fridge.name %></td>
        <td><%= fridge.description %></td>
        <td><%= fridge.latitude %></td>
        <td><%= fridge.longitude %></td>
        <td><%= fridge.update_count %></td>
        <td><%= fridge.created_by %></td>
        <td><%= fridge.created_api_caller %></td>
        <td><%= fridge.updated_by %></td>
        <td><%= fridge.updated_api_caller %></td>
        <td><%= link_to 'Show', fridge %></td>
        <td><%= link_to 'Edit', edit_fridge_path(fridge) %></td>
        <td><%= link_to 'Destroy', fridge, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fridge', new_fridge_path %>
