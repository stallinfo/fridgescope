<% provide(:title, "Log in") %> 

<div class="pageContainer">
      <div class="serviceName">Fridge Scope</div>
      <div class="siteName">System administrator site</div>
      <div class="inputLayoutContainer">
        <div class="iconContainer">
          <div class="iconLayout">
            <img
              class="icon"
              src="data:image/png;base64,"
            />
          </div>
        </div>
        <div class="inputContainer">
            <%= form_with(url: administrator_login_path, scope: :administrator_session, local: true) do |f| %>
            <%= f.label :identify ,"Your ID"%>
            <%= f.text_field :identify, class: 'form-control' %>
            
            <%= f.label :password %>
            <%= f.password_field :password, class: 'form-control' %>

            <%= f.submit "Sign in", class: "btn btn-primary" %> 
            <% end %>        
        </div>
      </div>
</div>