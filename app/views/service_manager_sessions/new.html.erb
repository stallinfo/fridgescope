<% provide(:title, "Log in") %> 

<div class="serviceManagerSessions-pageContainer">
      <div class="serviceName">Fridge Scope</div>
      <div class="siteName">Service manager site</div>
      <div class="inputLayoutContainer">
        <div class="iconContainer">
          <div class="iconLayout">
            <img
              class="icon"
              src="data:image/png;base64,"
            />
          </div>
        </div>
        <div class="inputContainer">
            <%= form_with(url: service_manager_login_path, scope: :service_manager_session, local: true) do |f| %>
            <%= f.label :identify ,"Connection Phrase"%>
            <%= f.text_field :connection_phrase, class: 'form-control' %>

            <%= f.label :identify ,"Your ID"%>
            <%= f.text_field :identify, class: 'form-control' %>
            
            <%= f.label :password %>
            <%= f.password_field :password, class: 'form-control' %>

            <%= f.submit "Sign in", class: "btn btn-primary" %> 
            <% end %>        
        </div>
      </div>
</div>